{
  "$defs": {
    "SearchItem": {
      "description": "Concise search result for quick discovery.",
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "kind": {
          "$ref": "#/$defs/SymbolKind"
        },
        "file_path": {
          "format": "path",
          "title": "File Path",
          "type": "string"
        },
        "line": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Line"
        },
        "container": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Container"
        }
      },
      "required": [
        "name",
        "kind",
        "file_path"
      ],
      "title": "SearchItem",
      "type": "object"
    },
    "SearchRequest": {
      "description": "Searches for symbols across the workspace by name pattern.\n\nReturns concise results for quick discovery. Use SymbolRequest\nto get detailed information about a specific symbol.",
      "properties": {
        "max_items": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Max Items"
        },
        "start_index": {
          "default": 0,
          "title": "Start Index",
          "type": "integer"
        },
        "pagination_id": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Pagination Id"
        },
        "query": {
          "title": "Query",
          "type": "string"
        },
        "kinds": {
          "anyOf": [
            {
              "items": {
                "$ref": "#/$defs/SymbolKind"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Kinds"
        }
      },
      "required": [
        "query"
      ],
      "title": "SearchRequest",
      "type": "object"
    },
    "SymbolKind": {
      "enum": [
        "file",
        "module",
        "namespace",
        "package",
        "class",
        "method",
        "property",
        "field",
        "constructor",
        "enum",
        "interface",
        "function",
        "variable",
        "constant",
        "string",
        "number",
        "boolean",
        "array",
        "object",
        "key",
        "null",
        "enumMember",
        "struct",
        "event",
        "operator",
        "typeParameter"
      ],
      "title": "SymbolKind",
      "type": "string"
    }
  },
  "markdown": "\n# Search: `{{ request.query }}`\n{% if total != nil -%}\nFound {{ total }} results | Showing: {{ items.size }}{% if max_items != nil %} (Offset: {{ start_index }}, Limit: {{ max_items }}){% endif %}\n{%- endif %}\n\n{% if items.size == 0 -%}\nNo matches found.\n{%- else -%}\n{%- for item in items %}\n- `{{ item.name }}` ({{ item.kind }}): `{{ item.file_path }}{% if item.line != nil %}:{{ item.line }}{% endif %}`{% if item.container != nil %} (in `{{ item.container }}`){% endif %}\n{%- endfor %}\n\n{% if has_more -%}\n---\n> [!TIP]\n> More results available.\n> To see more, use: `pagination_id=\"{{ pagination_id }}\"`, `start_index={{ start_index | plus: items.size }}`\n{%- endif %}\n{%- endif %}\n",
  "properties": {
    "start_index": {
      "title": "Start Index",
      "type": "integer"
    },
    "max_items": {
      "title": "Max Items",
      "type": "integer"
    },
    "total": {
      "title": "Total",
      "type": "integer"
    },
    "has_more": {
      "title": "Has More",
      "type": "boolean"
    },
    "pagination_id": {
      "title": "Pagination Id",
      "type": "string"
    },
    "request": {
      "$ref": "#/$defs/SearchRequest"
    },
    "items": {
      "items": {
        "$ref": "#/$defs/SearchItem"
      },
      "title": "Items",
      "type": "array"
    }
  },
  "required": [
    "start_index",
    "max_items",
    "total",
    "has_more",
    "pagination_id",
    "request",
    "items"
  ],
  "title": "SearchResponse",
  "type": "object"
}
